<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
    <title>Custom Layout</title>
  </head>
  <body>
    <div class="container-fluid py-4">
      <div class="row">
        <!-- Left Column -->
        <div class="col-lg-3" style="max-width: 25%; min-width: 20%;">
          <div class="card mb-4 shadow-sm" style="height: 300px;">
            <div class="card-body text-center">
              <h5>Large Card</h5>
            </div>
          </div>
          <div class="card mb-4 shadow-sm" style="height: 150px;">
            <div class="card-body text-center">
              <h5>Small Card</h5>
            </div>
          </div>
          <div class="card shadow-sm" style="height: 200px;">
            <div class="card-body text-center">
              <h5>Medium Card</h5>
            </div>
          </div>
        </div>

        <!-- Middle Column -->
        <div class="col-lg-6" style="max-width: 50%; min-width: 40%;">
          <div class="row">
            <div class="col-md-6">
              <div class="card mb-4 shadow-sm" style="height: 150px;">
                <div class="card-body text-center">
                  <h5>Card 1</h5>
                </div>
              </div>
            </div>
            <div class="col-md-6">
              <div class="card mb-4 shadow-sm" style="height: 150px;">
                <div class="card-body text-center">
                  <h5>Card 2</h5>
                </div>
              </div>
            </div>
          </div>
          <div class="card mb-4 shadow-sm" style="height: 200px;">
            <div class="card-body text-center">
              <h5>Wide Card</h5>
            </div>
          </div>
          <div class="card shadow-sm" style="height: 300px;">
            <div class="card-body text-center">
              <h5>Large Card</h5>
            </div>
          </div>
        </div>

        <!-- Right Column -->
        <div class="col-lg-3" style="max-width: 25%; min-width: 20%;">
          <div class="card mb-4 shadow-sm" style="height: 150px;">
            <div class="card-body text-center">
              <h5>Top Card</h5>
            </div>
          </div>
          <div class="card mb-4 shadow-sm" style="height: 200px;">
            <div class="card-body text-center">
              <h5>Middle Card</h5>
            </div>
          </div>
          <div class="card shadow-sm" style="height: 250px;">
            <div class="card-body text-center">
              <h5>Bottom Card</h5>
            </div>
          </div>
        </div>

      </div>
    </div>

    <div style="background-color: #000; color: #fff; text-align: center; padding: 10px 0; position: fixed; bottom: 0; left: 0; right: 0; z-index: 1000;">
      Career
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
